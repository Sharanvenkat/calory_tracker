    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Calories</title>
    <style>
        .a {
            height: 100%;
            width: 100%;
            background-color: bisque;
        }
        .aa{
            height:100%;
            width:100px;
            border:2px solid black;
            float: left;
            text-align: center;
        }
        .b{
            height: 20px;
            width: 100%;
            background-color: orangered;
            padding: 15px;
        }
        .c{
            height: 100%;
            width:800px;
            background-color: silver;
            margin-left: 200px;
            float:left;
        }
        .d{
            text-align: center;
            font-size: large;
            margin-top: 20px;
        }
        .e{
            background-color: orangered;
            margin-top: 30px;
            color: white;
            margin-left: 200px;
        }
        .ea{
            margin-top: 30px;
            margin-left: 200px;
        }
        .ee{
