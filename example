                document.getElementById('addFoodButton').addEventListener('click', function() {
                    document.getElementById('dropdownContainer').style.display = 'block';
                });
        
                document.getElementById('foodDropdown').addEventListener('change', function() {
                    document.getElementById('foodForm').style.display = 'block';
                });
        
                document.getElementById('addFood').addEventListener('click', function() {
                    let name = document.getElementById('Name').value;
                    let calories = document.getElementById('Cal').value;
                    let imgUrl = document.getElementById('Img').value;
        
                    let displaySection = document.getElementById('displaySection');
                    displaySection.innerHTML = `
                        <div class="img"><img src="${imgUrl}" alt="${name}" style="width:150px;height:125px;"></div>
                        <div class="write">${name}<br>Calories-${calories}</div>
                    `;
                });
            </script>
