            <div class="f" id="foodForm">
                <label for="Name">Name</label>
                <input type="text" id="Name" placeholder="Example Name"><br>
                <label for="Cal">Calories</label>
                <input type="text" id="Cal" placeholder="250"><br>
                <label for="Img">Image</label>
                <input type="text" id="Img" placeholder="Image URL"><br>
                <input type="button" value="Add Food" class="ee" id="addFood"><br>
            </div>
            <div id="displayContainer">
                <div class="display ea">
                    <div class="img"><img src="apple.jpeg" alt="Apple" style="width:150px;height:125px;"></div>
                    <div class="write">Apple<br>Calories-50
                        <div class="counter">
                            <input type="number" value="0" min="0">
                            <button class="addCount">Add</button>
                        </div>
                    </div>
                </div>
                <div class="display ea">
                    <div class="img"><img src="Grapes.jpeg" alt="Grapes" style="width:150px;height:125px;"></div>
                    <div class="write">Grapes<br>Calories-20
