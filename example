            overflow: hidden;
        }
        .img{
            height:125px;
            width:150px;
            border:2px solid black;
            margin-left: 10px;
            float: left;
            margin-top: 10px;
        }
        .write{
            height:125px;
            width:260px;
            border:2px solid black;
            margin-left: 10px;
            float: left;
            margin-top: 10px;
            text-align: center;
            font-size: larger;
            position: relative;
        }
        .content{
            padding:20px;
            text-align: center;
        }
        .counter {
            position: absolute;
            bottom: 10px;
            left: 50%;
            transform: translateX(-50%);
        }
        .counter input[type="number"] {
            width: 50px;
        }
        .counter button {
            margin-top: 5px;
        }
    </style>
</head>
<body>
    <div class="a">
        <div class="b"><span style="color: white;font-size: larger;">Calory Tracker</span></div>

        <div class="c">

        <div class="d">
            <label>Search</label><br>
            <input type="text" id="search"><br>
        </div>
        <div>
            <input type="button" value="Add New Food" class="e" id="addFoodButton"><br>
        </div>
        <div id="dropdownContainer" class="ea" style="display: none;">
            <select class="dropdown" id="foodDropdown">
                <option value="fruit">Fruit</option>
                <option value="vegetable">Vegetable</option>
                <option value="snack">Snack</option>
            </select>
