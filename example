            </div>
        </div>
        <div class="aa">TODAY'S FOOD
            <div id="todaysFoods"></div>
            <div id="totalCalories">Total Calories: 0</div>
        </div>
    </div>

    <script>
        document.getElementById('addFoodButton').addEventListener('click', function() {
            document.getElementById('dropdownContainer').style.display = 'block';
        });

        document.getElementById('foodDropdown').addEventListener('change', function() {
            document.getElementById('foodForm').style.display = 'block';
        });

        document.getElementById('addFood').addEventListener('click', function() {
            let name = document.getElementById('Name').value;
            let calories = document.getElementById('Cal').value;
            let imgUrl = document.getElementById('Img').value;

            let displayContainer = document.getElementById('displayContainer');
            let newFoodItem = document.createElement('div');
            newFoodItem.className = 'display ea';
            newFoodItem.innerHTML = `
                <div class="img"><img src="${imgUrl}" alt="${name}" style="width:150px;height:125px;"></div>
                <div class="write">${name}<br>Calories-${calories}
                    <div class="counter">
                        <input type="number" value="0" min="0">
                        <button class="addCount">Add</button>
                    </div>
                </div>
            `;
            displayContainer.appendChild(newFoodItem);
            document.getElementById('Name').value = '';
            document.getElementById('Cal').value = '';
            document.getElementById('Img').value = '';
        });

        document.getElementById('search').addEventListener('input', function() {
            let query = this.value.toLowerCase();
            let foodItems = document.querySelectorAll('#displayContainer .display');
            
            foodItems.forEach(function(item) {
                let itemName = item.querySelector('.write').textContent.toLowerCase();
                if (itemName.includes(query)) {
                    item.style.display = 'block';
                } else {
                    item.style.display = 'none';
                }
            });
        });

        document.getElementById('displayContainer').addEventListener('click', function(event) {
            if (event.target.classList.contains('addCount')) {
                let counterInput = event.target.previousElementSibling;
                counterInput.value = parseInt(counterInput.value) + 1;

                let foodItem = event.target.closest('.display');
                let foodName = foodItem.querySelector('.write').childNodes[0].textContent.trim();
                let foodCalories = parseInt(foodItem.querySelector('.write').childNodes[2].textContent.replace('Calories-', '').trim());
                let totalCalories = foodCalories * parseInt(counterInput.value);

                let todaysFoodsContainer = document.getElementById('todaysFoods');
                let existingFoodItem = document.getElementById(foodName);

                if (existingFoodItem) {
                    existingFoodItem.innerHTML = `${foodName}: ${totalCalories} calories (${counterInput.value} servings)`;
                } else {
                    let newTodaysFoodItem = document.createElement('div');
                    newTodaysFoodItem.id = foodName;
                    newTodaysFoodItem.innerHTML = `${foodName}: ${totalCalories} calories (${counterInput.value} servings)`;
                    todaysFoodsContainer.appendChild(newTodaysFoodItem);
                }

                updateTotalCalories();
            }
        });

        function updateTotalCalories() {
            let totalCalories = 0;
            let todaysFoods = document.querySelectorAll('#todaysFoods > div');
            todaysFoods.forEach(function(foodItem) {
                let foodText = foodItem.innerHTML;
                let caloriesMatch = foodText.match(/(\d+) calories/);
                if (caloriesMatch) {
                    totalCalories += parseInt(caloriesMatch[1]);
                }
            });
            document.getElementById('totalCalories').textContent = 'Total Calories: ' + totalCalories;
        }
    </script>
</body>
</html>


